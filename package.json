{
  "name": "form-generator",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "build:render": "vue-cli-service build --target lib --name form-gen-render --dest ./src/components/render/lib/ ./src/components/render/render.js",
    "build:parser": "vue-cli-service build --target lib --name form-gen-parser --dest ./src/components/parser/lib/ ./src/components/parser/index.js",
    "build:tinymce": "vue-cli-service build --target lib --name form-gen-tinymce --dest ./src/components/tinymce/lib/ ./src/components/tinymce/index.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/parser": "^7.7.4",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "axios": "^0.19.2",
    "clipboard": "^2.0.4",
    "core-js": "^3.25.5",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "file-saver": "^2.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "sass": "1.32.7",
    "sass-loader": "10.0.0",
    "sass-resources-loader": "2.2.5",
    "throttle-debounce": "^2.1.0",
    "validate-commit-msg": "^2.14.0",
    "vue": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-service": "~4.4.0",
    "svg-sprite-loader": "^4.1.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{html,vue}": [
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  },
  "homepage": "https://jakhuang.github.io/form-generator"
}
